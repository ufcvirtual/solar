- if @schedules.any?
  - @schedules.each do |schedule|
    %div= link_to (schedule['start_date'] == @date ? t('agendas.begin_of') : (schedule['end_date'] == @date ? t('agendas.end_of'): '')) + schedule['name'], list_agendas_path(bread: 'menu_agenda', start_date: schedule['start_date'], end_date: schedule['end_date']), { class: 'agenda_ajax can-update-menu', 'data-menu-bread' => 'menu_agenda' }
- else
  %span.agenda_ajax= t(:no_events)
