%div
  - @schedule.each do |schedule|
    - if @link
      %div= link_to schedule[:name], list_agendas_path(bread: 'menu_agenda', start_date: schedule[:start_date], end_date: schedule[:end_date]), {class: 'agenda_ajax can-update-menu', 'data-menu-bread' => 'menu_agenda'}
    - else
      %span.agenda_ajax= schedule[:name]
  - if @schedule.first.nil?
    %span.agenda_ajax= t(:no_events)
  - else
    = link_to t(:curriculum_access_more), list_agendas_path(bread: 'menu_agenda'), {class: 'schedule_link_more can-update-menu', 'data-menu-bread' => 'menu_agenda'}
