#block_schedule.block_small.portlet
  .block_title
    %h2.agenda_title
      %i.icon-calendar{:'aria-hidden' =>'true'}
      = t(:curriculum_access_agenda)
  .calendar#agenda{url_for: agendas_path}
  .calendar_events

:javascript

  $(function() {
    var dates_with_events = eval(#{@scheduled_events.as_json});
    showAgenda(dates_with_events);
  });
